# Single DATABASE_URL connection string (preferred). URL-encode any special
# characters in the password (e.g. @ -> %40).
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/moviesdb?sslmode=disable
PG_HOST=localhost
PG_PORT=5432
PG_USER=postgres
PG_PASSWORD=postgres
PG_DB=moviesdb

# Optional: HTTP listen address
HTTP_ADDR=:8080
